{#
/**
 * @file
 * Default theme implementation for X share button.
 *
 * Available variables:
 *  - url: The url of the page to be shared.
 *  - content: The button's image or other content to appear on the page. (default: image)
 *
 */
#}
<a href="https://x.com/intent/tweet?text=&url={{ url | url_encode }}" target="_blank" rel="noreferrer">{{ content }}</a>
